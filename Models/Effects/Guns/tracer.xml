<?xml version="1.0" encoding="UTF-8"?>

<!-- Red Tracer -->
<PropertyList>
  
  <path>tracer.ac</path>
  
  <animation>
    <enable-hot type="bool">false</enable-hot>
  </animation>

  
  <animation>
  <type>scale</type>
  <object-name>tracer</object-name>
    <x-offset>0.3</x-offset>
    <y-offset>0.3</y-offset>
    <z-offset>0.3</z-offset>
  </animation>
  
  <animation>
    <type>material</type>
    <object-name>tracer</object-name>
    <emission>
      <factor>1</factor>
      <offset>1</offset>
      <red>   1 </red>
      <green> 0.333 </green>
      <blue>  0.333 </blue>
    </emission>
  </animation>
  
</PropertyList>