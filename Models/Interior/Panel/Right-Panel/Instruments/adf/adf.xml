<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>adf.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>fond</object-name>
    <object-name>visses</object-name>
    <object-name>info</object-name>
    <object-name>numL0</object-name>
    <object-name>numL1</object-name>
    <object-name>numL2</object-name>
    <object-name>numL3</object-name>
    <object-name>numR0</object-name>
    <object-name>numR1</object-name>
    <object-name>numR2</object-name>
    <object-name>numR3</object-name>
    
    <!-- Objets transparents -->
  </animation>

  <animation>
    <type>material</type>
    <object-name>info</object-name>
    <object-name>numL0</object-name>
    <object-name>numL1</object-name>
    <object-name>numL2</object-name>
    <object-name>numL3</object-name>
    <object-name>numR0</object-name>
    <object-name>numR1</object-name>
    <object-name>numR2</object-name>
    <object-name>numR3</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>    
  
</PropertyList>
