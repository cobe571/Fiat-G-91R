<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>light.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>fond</object-name>
    <object-name>visses</object-name>
    <object-name>info</object-name>
    <object-name>knob</object-name>
    <object-name>knob2</object-name>
    <object-name>knob3</object-name>
    <object-name>switch1</object-name>
    <object-name>switch2</object-name>
    <object-name>switch3</object-name>
    <object-name>switch4</object-name>
    
    <!-- Objets transparents -->
  </animation>

  <animation>
    <type>material</type>
    <object-name>info</object-name>
    <object-name>knob</object-name>
    <object-name>knob2</object-name>
    <object-name>knob3</object-name>
    <object-name>switch1</object-name>
    <object-name>switch2</object-name>
    <object-name>switch3</object-name>
    <object-name>switch4</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>    
  
</PropertyList>
