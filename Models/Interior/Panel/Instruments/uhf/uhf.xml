<?xml version="1.0" encoding="ISO-8859-1"?> 

<PropertyList>

  <path>uhf.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>num0</object-name>
    <object-name>num1</object-name>
    <object-name>num2</object-name>
    <object-name>num3</object-name>
    <object-name>num4</object-name>
    <object-name>num5</object-name>
    <object-name>dot</object-name>
    
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>num0</object-name>
    <object-name>num1</object-name>
    <object-name>num2</object-name>
    <object-name>num3</object-name>
    <object-name>num4</object-name>
    <object-name>num5</object-name>
    <object-name>dot</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>

  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Aircraft/Aermacchi-MB-339/Models/Effects/Glass/glass</inherits-from>
    <object-name>vitre</object-name>
  </effect>
  
  <animation>
    <type>textranslate</type>
    <object-name>num5</object-name>
    <property>/instrumentation/comm[0]/frequencies/selected-mhz</property>
    <factor>0.001</factor>
    <step type="double">100</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>textranslate</type>
    <object-name>num4</object-name>
    <property>/instrumentation/comm[0]/frequencies/selected-mhz</property>
    <factor>0.01</factor>
    <step type="double">10</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>textranslate</type>
    <object-name>num3</object-name>
    <property>/instrumentation/comm[0]/frequencies/selected-mhz</property>
    <factor>0.1</factor>
    <step type="double">1</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>textranslate</type>
    <object-name>num2</object-name>
    <property>/instrumentation/comm[0]/frequencies/selected-mhz</property>
    <factor>1</factor>
    <step type="double">0.1</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
</PropertyList>
